package components

import "github.com/uiansol/zentube/internal/entities"

templ SearchResults(videos []entities.Video) {
	<div id="results" style="margin-top: 2rem;">
		if len(videos) == 0 {
			<p>No results found.</p>
		} else {
			for _, v := range videos {
				@VideoResult(v)
			}
		}
	</div>
}
